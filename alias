
# QoL
alias python=python3
alias pip=pip3
alias nano=vim
alias tstart='source ~/.bashrc'
# Git
alias gs="git status"
alias gf="git fetch"
alias gt="git tpush"
alias gp="git pull"
# Docker
alias dockerssh='docker exec -it $1 /bin/bash'

# system
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'h
